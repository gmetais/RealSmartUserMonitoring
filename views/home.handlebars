<div class="content">
    <div class="grid header">
        <div class="col-1-1 title">
            <h1>REAL <b>SMART</b> USER MONITORING</h1>
            <span class="filters"></span>
        </div>
    </div>
    <div class="grid">
        <div class="col-1-4 smallGauge">
            <div id="uxGauge"></div>
        </div>
        <div class="col-1-4">
            <div class="counts">
                <div>Visits</div>
                <div class="visits">{{generalMetrics.nbVisits}}</div>
                <div>Pageviews</div>
                <div class="pageviews">{{generalMetrics.nbPages}}</div>
            </div>
        </div>
        <div class="col-1-4">
            <div class="pagesPerVisit">
                <div>Pages per visit</div>
                <div class="number">{{generalMetrics.pagesPerVisit}}</div>
            </div>
        </div>
        <div class="col-1-4">
            <div class="bounce">
                <div>Bounce Rate</div>
                <div class="number">{{generalMetrics.bounceRate}}%</div>
            </div>
        </div>
    </div>
    <div class="grid">
        <div class="col-9-12 timingsChart">
            <div id="timingsChart"></div>
        </div>
        <div class="col-3-12">
            <div class="what">
                <div>Server time</div>
                <div class="number">10%</div>
            </div>
        </div>
    </div>
    <div class="grid">
        <div class="col-1-2 smallChart">
            <div id="firstPageLoadTimeChart"></div>
        </div>
        <div class="col-1-2 smallChart">
            <div id="loadTimeChart"></div>
        </div>
    </div>
    <div class="grid footer">
        <div class="col-1-1">
            <span><a href="https://github.com/gmetais/RealSmartUserMonitoring">RSUM on GitHub</a></span>
        </div>
    </div>
</div>
<script>
    var rsumResults = {{{stringified}}};
</script>